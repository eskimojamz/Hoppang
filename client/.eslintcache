pen, setMenuOpen] = useState(false)\n    const[profileOpen, setProfileOpen] = useState(false)\n    const { user, isAuthenticated } = useAuth0()\n    console.log(user)\n\n    const Nav = (props) => {\n        return (\n        <nav className=\"nav\">\n            {props.children}\n        </nav>\n        )\n    }\n\n    const ProfileMenu = () => {\n        return (\n        <div className=\"profile-menu\">\n            <h5>Signed in as: </h5>\n            <h5>{user.nickname}</h5>\n            <Link to=\"/profile\"><button className=\"profile-btn\">My Profile</button></Link>\n            <LogoutButton />\n        </div>\n        )\n    }\n\n    return (\n        <>\n        <Nav>\n            <div className=\"nav-left\">\n                <Link to='/'><img className=\"logo\" src={logo} /></Link>\n                <h1>Hoppang</h1>\n            </div>\n\n            <MenuButton menuOpen={menuOpen} toggle={() => setMenuOpen(!menuOpen)} />\n\n            {menuOpen && \n            <div className=\"mobile-menu\">\n                <div className=\"mobile-menu-top\">\n                <Link to='/listings'><button className=\"listings-nav-btn-mobile\">Listings</button></Link>\n                <Link to='/form'><button className=\"sell-nav-btn-mobile\">Sell Now</button></Link>\n                </div>\n                {isAuthenticated\n                    ? (\n                    <>\n                    <div className=\"mobile-menu-profile\">\n                        <div className=\"mobile-menu-profile-left\">\n                            <img className=\"profile-mobile\" src={user?.picture} />\n                        </div>\n                        <div className=\"mobile-menu-profile-right\">\n                            <h5>Signed in as: </h5>\n                            <h4>{user.nickname}</h4>\n                        </div>\n                    </div>\n                    <div className=\"mobile-menu-bottom-logout\">\n                        <Link to=\"/profile\"><button className=\"profile-btn-mobile\">My Profile</button></Link>\n                        <LogoutButton />\n                    </div>\n                    </>\n                    )\n                    : <LoginButton />\n                }\n                \n            </div>\n            }\n\n            <div className=\"nav-right-desktop\">\n                <Link to='/listings'><button className=\"listings-nav-btn\">Listings</button></Link>\n                <Link to='/form'><button className=\"sell-nav-btn\">Sell Now</button></Link>\n                \n                {isAuthenticated\n                    ? (\n                    <>\n                    <img className=\"profile\" src={user?.picture} onClick={() => setProfileOpen(!profileOpen)} />\n                    {profileOpen && (\n                    <ProfileMenu>\n                    </ProfileMenu>)}\n                    </>\n                    )\n                    : <LoginButton />\n                }\n            </div>\n        </Nav>\n        </>\n\n    )\n}\n\nexport default Navbar\n\n\n\n","/Users/jameskim/Documents/GitHub/Hoppang/client/src/actions/listings.js",[],"/Users/jameskim/Documents/GitHub/Hoppang/client/src/reducers/currentId.js",[],"/Users/jameskim/Documents/GitHub/Hoppang/client/src/reducers/listings.js",[],"/Users/jameskim/Documents/GitHub/Hoppang/client/src/components/Listing.js",["120","121","122","123","124","125"],"import React, { useState } from 'react'\nimport { useSelector, useDispatch } from 'react-redux'\nimport { Link } from 'react-router-dom'\nimport { setCurrentId, likeListing } from '../actions/listings'\nimport comment from '../assets/comments.svg'\nimport like from '../assets/like.svg'\n\nconst Listing = ({listing, user}) => {\n    const id = listing._id\n    const userId = user?.sub\n    const dispatch = useDispatch()\n\n    const handleLike = () => {\n        dispatch(likeListing(id, userId))\n    }\n    console.log(listing)\n    console.log(user)\n    return (\n        <div className=\"listing\">\n            <div className=\"listing-img-div\">\n                <img src={listing.selectedFile[0].base64} />\n            </div>\n            <div className=\"listing-tooltip\">\n                \n                    <button className=\"listing-tooltip-like p-1\" onClick={handleLike}>\n                        <img src={like} />\n                    </button>\n                    <span><h5 className=\"p-1\">{listing.likers.length}</h5></span>\n                \n                \n                    <button className=\"listing-tooltip-comment p-1\">\n                    <img src={comment} />\n                    </button>\n                    <span><h5 className=\"p-1\">{listing.commentCount}</h5></span>\n                \n            </div>\n            <div className=\"listing-title\">\n                <h3>{listing.title}</h3>\n            </div>\n            <div className=\"listing-price\">\n                <h3>${listing.price}</h3>\n            </div>\n            <div className=\"listing-btn\">\n                <Link \n                    to={{\n                        pathname: `/listings:${id}`,\n                        state: {listing}\n                    }}>\n                    <button className=\"button-primary\">Details</button>\n                </Link>\n            </div>\n        </div>\n    )\n}\n\nexport default Listing\n","/Users/jameskim/Documents/GitHub/Hoppang/client/src/components/LoginButton.js",[],"/Users/jameskim/Documents/GitHub/Hoppang/client/src/components/LogoutButton.js",[],"/Users/jameskim/Documents/GitHub/Hoppang/client/src/components/ListingForm.js",["126"],"import React, { useState, useEffect } from 'react';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { Redirect } from 'react-router-dom';\nimport FileBase from 'react-file-base64';\nimport { useAuth0 } from '@auth0/auth0-react'\n\n\nimport { createListing, updateListing, setCurrentId } from '../actions/listings';\n\nconst ListingForm = () 