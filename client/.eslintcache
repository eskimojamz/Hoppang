map(userListing => \n          <Listing listing={userListing} />\n        )}\n      </div>\n    </motion.div>\n    )\n  )\n}\n\nexport default Profile","/Users/jameskim/Documents/GitHub/Hoppang/client/src/routes/ListingInfoPage.js",["98","99","100","101","102","103","104"],"/Users/jameskim/Documents/GitHub/Hoppang/client/src/routes/FormPage.js",["105"],"import React from 'react'\nimport ListingForm from '../components/ListingForm'\n\nimport { motion, AnimateSharedLayout} from 'framer-motion'\n\nconst FormPage = () => {\n    const containerVariants = {\n        hidden: { \n          opacity: 0, \n          y: '100vw',\n        },\n        visible: { \n          opacity: 1, \n          y: 0,\n          transition: { duration: 1 }\n        },\n        exit: {\n            x: \"-100vh\",\n            transition: { duration: 1, ease: 'easeInOut' }\n        }\n    };\n\n    return (\n        <motion.div className=\"grid-12\"\n            variants={containerVariants}\n            initial=\"hidden\"\n            animate=\"visible\"\n            exit=\"exit\"\n        >\n            <ListingForm ></ListingForm>\n        </motion.div>\n    )\n}\n\nexport default FormPage\n","/Users/jameskim/Documents/GitHub/Hoppang/client/src/components/Navbar.js",["106","107","108","109"],"import { useState } from 'react'\nimport { motion } from 'framer-motion'\nimport LoginButton from './LoginButton'\nimport LogoutButton from './LogoutButton'\nimport { useAuth0 } from '@auth0/auth0-react'\nimport { Link } from 'react-router-dom'\nimport logo from '../assets/HoppangLogo.svg'\nimport MenuButton from './MenuButton'\n\nconst Navbar = () => {\n    const[menuOpen, setMenuOpen] = useState(false)\n    const[profileOpen, setProfileOpen] = useState(false)\n    const { user, isAuthenticated } = useAuth0()\n    console.log(user)\n\n    const Nav = (props) => {\n        return (\n        <nav className=\"nav\">\n            {props.children}\n        </nav>\n        )\n    }\n\n    const ProfileMenu = () => {\n        return (\n        <div className=\"profile-menu\">\n            <h5>Signed in as: </h5>\n            <h5>{user.nickname}</h5>\n            <Link to=\"/profile\"><button className=\"profile-btn\">My Profile</button></Link>\n            <LogoutButton />\n        </div>\n        )\n    }\n\n    return (\n        <>\n        <Nav>\n            <div className=\"nav-left\">\n                <Link to='/'><img className=\"logo\" src={logo} /></Link>\n                <h1>Hoppang</h1>\n            </div>\n\n            <MenuButton menuOpen={menuOpen} toggle={() => setMenuOpen(!menuOpen)} />\n\n            {menuOpen && \n            <div className=\"mobile-menu\">\n                <div className=\"mobile-menu-top\">\n                <Link to='/listings'><button className=\"listings-nav-btn-mobile\">Listings</button></Link>\n                <Link to='/form'><button className=\"sell-nav-btn-mobile\">Sell Now</button></Link>\n                </div>\n                {isAuthenticated\n                    ? (\n                    <>\n                    <div className=\"mobile-menu-profile\">\n                        <div className=\"mobile-menu-profile-left\">\n                            <img className=\"profile-mobile\" src={user?.picture} />\n                        </div>\n                        <div className=\"mobile-menu-profile-right\">\n                            <h5>Signed in as: </h5>\n                            <h4>{user.nickname}</h4>\n                        </div>\n                    </div>\n                    <div className=\"mobile-menu-bottom-logout\">\n                        <Link to=\"/profile\"><button className=\"profile-btn-mobile\">My Profile</button></Link>\n                        <LogoutButton />\n                    </div>\n                    </>\n                    )\n                    : <LoginButton />\n                }\n                \n            </div>\n            }\n\n            <div className=\"nav-right-desktop\">\n                <Link to='/listings'><button className=\"listings-nav-btn\">Listings</button></Link>\n                <Link to='/form'><button className=\"sell-nav-btn\">Sell Now</button></Link>\n                \n                {isAuthenticated\n                    ? (\n                    <>\n                    <img className=\"profile\" src={user?.picture} onClick={() => setProfileOpen(!profileOpen)} />\n                    {profileOpen && (\n                    <ProfileMenu>\n                    </ProfileMenu>)}\n                    </>\n                    )\n                    : <LoginButton />\n                }\n            </div>\n        </Nav>\n        </>\n\n    )\n}\n\nexport default Navbar\n\n\n\n","/Users/jameskim/Documents/GitHub/Hoppang/client/src/actions/listings.js",[],"/Users/jameskim/Documents/GitHub/Hoppang/client/src/reducers/currentId.js",[],"/Users/jameskim/Documents/GitHub/Hoppang/client/src/reducers/listings.js",[],"/Users/jameskim/Documents/GitHub/Hoppang/client/src/components/Listing.js",["110","111","112","113"],"/Users/jameskim/Documents/GitHub/Hoppang/client/src/components/LoginButton.js",[],"/Users/jameskim/Documents/GitHub/Hoppang/client/src/components/LogoutButton.js",[],"/Users/jameskim/Documents/GitHub/Hoppang/client/src/components/ListingForm.js",["114"],"import React, { useState, useEffect } from 'react';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { Redirect } from 'react-router-dom';\nimport FileBase from 'react-file-base64';\nimport { useAuth0 } from '@auth0/auth0-react'\n\n\nimport { createListing, updateListing, setCurrentId } from '../actions/listings';\n\nconst ListingForm = () => {\n    const { user } = useAuth0()\n    console.log(user)\n    \n    const [listingData, setListingData] = useState({ title: '', description: '', price: '', selectedFile: '', likeCount: 0, commentCount: 0})\n    const currentId = useSelector(state => state.currentId)\n    const currentListing = useSelector(state => currentId ? state.listings.find(listing => listing._id === currentId) : null)\n    const dispatch = useDispatch()\n    const [redirect, setRedirect] = useState(false)\n    const [redirectId, setRedirectId] = useState(null)\n    console.log(redirectId)\n    console.log(currentId)\n    console.log(currentListing)\n    useEffect(() => {\n        if(currentListing) setListingData(currentListing)\n    }, [currentListing])\n    useEffect(() => {\n        if(currentId) setRedirectId(currentId)\n    }, [])\n    const clear = () => {\n        dispatch(setCurrentId(null))\n        setListingData({ title: '', description: '', price: '', selectedFile: '' })\n    }\n    \n    const handleSubmit = (e) => {\n        e.preventDefault()\n        if (currentId) {\n            dispatch(updateListing(currentId, listingData))\n            clear()\n            console.log(redirectId)\n            setRedirect(true)\n        } else {\n            dispatch(createListing({...listingData, creator: user?.sub, creatorName: user?.nickname, creatorImg: user?.picture }))\n            clear()\n            setRedirect(true)\n        }\n    }\n\n    return (\n        <div className=\"form\">\n        <h3>Create Listing</h3>\n        { redirect && <Redirect to={`/listings`} /> }\n        <form onSubmit={handleSubmit}>\n            <label>\n                <h4>Title:</h4>\n                <input type=\"text\" value={listingData.title} onChange={(e) => setListingData({ ...listingData, title: e.target.value })} />\n            </label>\n\n            <label>\n                <h4>Price:</h4>\n                <input type=\"text\" placeholder=\"$\" value={listingData.price} onChange={(e) => setListingData({ ...listingData, price: e.target.value })} />\n            </label>\n\n            <label>\n                <h4>Description:</h4>\n                <textarea value={listingData.description} onChange={(e) => setListingData({ ...listingData, description: e.target.value })}/>\n            </label>\n\n            <label className=\"file-input\">\n                <h4>Images:</h4>\n            <FileBase \n                type=\"file\" \n                multiple={true} \n                onDone={base64 => setListingData({ ...listingData, selectedFile: base64 })}\n            />\n            </label>\n            {/* <input type=\"file\" multiple={true} onChange={(event) => setListingData({ ...listingData, selectedFile: event.target.files[0] })}></input> */}\n            <br />\n            <br />\n            <button className=\"button-primary\" type=\"submit\" >\n            Submit\n            </button>\n        </form> \n        </div>\n    )\n}\n\nexport default ListingForm","/Users/jameskim/Documents/GitHub/Hoppang/client/src/components/MenuButton.js",[],"/Users/jameskim/Documents/GitHub/Hoppang/client/src/api/index.js",[],"/Users/jameskim/Documents/GitHub/Hoppang/client/src/components/Tooltip.js",["115","116"],"import React, { useState, useEffect, useRef } from \"react\";\nimport \"../css/Tooltip.css\";\n\nexport default function Modal({ content, toggle, setToggle }) {\n    const tooltip = useRef()\n\n    const handleClickOutside = e => {\n        if (tooltip.current.contains(e.target)) {\n            return\n            // inside click - do nothing\n        }\n        // outside click - toggle off\n        setToggle(false)\n    }\n\n    useEffect(() => {\n        if (toggle) {\n            // if open, add event listener for outside\n            document.addEventListener(\"mousedown\", handleClickOutside);\n        } else {\n            // else remove if closed\n            document.removeEventListener(\"mousedown\", handleClickOutside);\n        }\n        return () => {\n            // remove if closed \n            document.removeEventListener(\"mousedown\", handleClickOutside);\n        };\n        // on every toggle instance\n      }, [toggle]);\n    \n    return (\n        <>\n        {toggle && (\n            <div ref={tooltip} className=\"tooltip-content\">\n        